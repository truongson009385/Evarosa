<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/webfonts/font-awesome/css/all.css" />
    <link rel="stylesheet" href="~/css/BundlesCss.min.css" />
    <link rel="stylesheet" href="~/css/Style.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StyleMember.min.css" asp-append-version="true" />
</head>
<body>
    @await Component.InvokeAsync("Header")

    <main role="main">
        @RenderBody()
    </main>

    @await Component.InvokeAsync("Footer")

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/jqueryui/jquery-ui.min.js"></script>
    <script src="~/js/BundlesJs.min.js"></script>
    <script src="~/js/Script.js" asp-append-version="true"></script>
    <script src="~/js/ScriptMember.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
